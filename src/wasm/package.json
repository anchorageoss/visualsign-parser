{
  "name": "@anchorageoss/visualsign-parser",
  "version": "0.1.0",
  "description": "WebAssembly bindings for VisualSign transaction parser - convert raw blockchain transactions into human-readable format",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "visualsign_wasm_bg.wasm",
    "visualsign_wasm_bg.wasm.d.ts",
    "visualsign_wasm.js",
    "visualsign_wasm.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anchorageoss/visualsign-parser.git"
  },
  "license": "Apache-2.0",
  "keywords": [
    "blockchain",
    "transaction",
    "parser",
    "ethereum",
    "solana",
    "sui",
    "tron",
    "wasm",
    "webassembly",
    "visualsign"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "wasm-pack build --target bundler --out-dir pkg",
    "build:nodejs": "wasm-pack build --target nodejs --out-dir pkg-node",
    "build:web": "wasm-pack build --target web --out-dir pkg-web"
  },
  "devDependencies": {
    "wasm-pack": "^0.12.0"
  }
}
